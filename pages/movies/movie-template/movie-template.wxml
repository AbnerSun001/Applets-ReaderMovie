<import src="../star-template/star-template.wxml" />

<template name="movie-template">
  <view class="movie-container">
    <image src="{{imageUrl}}" class="movie-container-poster"></image>
    <text class="movie-container-name">{{title}}</text>
    <template is="star-template" data="{{stars:stars,average:average}}"></template>
  </view>
</template>